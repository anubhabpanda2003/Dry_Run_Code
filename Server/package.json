{
  "name": "code-dry-run-visualizer-server",
  "version": "1.0.0",
  "description": "Server for Code Dry Run Visualizer - A tool to visualize code execution flow",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "cd ../client && npm start",
    "client:build": "cd ../client && npm run build",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "heroku-postbuild": "npm install && npm run client:build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "code",
    "visualization",
    "dry-run",
    "education"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "java-parser": "^1.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
